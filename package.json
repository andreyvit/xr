{
  "name": "xr",
  "version": "0.1.13",
  "description": "Ultra-simple wrapper around XMLHttpRequest.",
  "main": "xr.js",
  "scripts": {
    "build": "babel  --stage 0 --optional runtime --optional bluebirdCoroutines --modules umd -o xr.js --loose all src/xr.js",
    "update-authors": "git log --format='%aN <%aE>' | sort -u | grep -v noreply > AUTHORS",
    "prepublish": "npm run build; npm run update-authors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radiosilence/xr"
  },
  "keywords": [
    "request",
    "xmlhttprequest",
    "json",
    "ajax"
  ],
  "author": "James Cleveland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radiosilence/xr/issues"
  },
  "homepage": "https://github.com/radiosilence/xr",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "eslint-config-airbnb": "0.0.8",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2"
  },
  "dependencies": {
    "babel-runtime": "^5.8.24"
  }
}
